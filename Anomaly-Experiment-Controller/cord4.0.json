{
    "hypervisors": {
	"children": [
	    "head1", 
	    "ambitious-farm.cord.lab"
	]
    }, 
    "hosts": {
        "hosts": [
            "head1", 
            "ambitious-farm.cord.lab"
        ], 
        "vars": {
            "ansible_ssh_private_key_file": "~/.ssh/id_rsa"
        }
    }, 
    "head1": {
        "hosts": [
            "head1"
        ], 
        "vars": {
            "iface": "em1", 
            "hostname": "head1", 
            "ansible_host": "head1", 
            "injector-endpoint": "head1:7888"
        }
    }, 
    "ambitious-farm.cord.lab": {
        "hosts": [
            "ambitious-farm.cord.lab"
        ], 
        "vars": {
            "iface": "em1", 
            "hostname": "ambitious-farm.cord.lab", 
            "ansible_host": "ambitious-farm.cord.lab", 
            "injector-endpoint": "ambitious-farm.cord.lab:7888"
        }
    }
}
